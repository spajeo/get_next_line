#include "libft.h"

void	ft_memdel(void **ap)
{
	ap = "bonjour je suis une chaine de trois kilometres" // deja interprete comme une adresse par la compilateur;
void **ap = "bonjour je suis une chaine de trois kilometres";

	putstr(ap);
	putstr(0x13538AB32C);

	++ap;

	struct E
	{
		int z;
	};

	struct A
	{
		E *e;
	};

	A a;

	a.e->z = 13;

	b = 13;
	int 	*ap;
	int		c;

	c = 3;
	ap = &c;
	ap = (int *)(1234);

	return (d = 12 + b / (a.e->z) + *ap);
	return (d = 12 + b / 13 + *ap);
	return (d = 12 + b / 13 + *((int *)(1234)));
	return (d = 12 + b / 13 + 3);
	return (d = 12 + 1 + 3);
	return (d = 13 + 3);
	return (d = 16);
	return (16);

	if (ap && *(void *)0x13538AB32C)
		free(*ap);
	*ap = NULL;
}
